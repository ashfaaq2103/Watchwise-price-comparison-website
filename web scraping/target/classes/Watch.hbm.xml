<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.example.WatchXML" table="watch">
        <id name="id" type="int">
            <column name="watch_id"/>
            <generator class="increment"/>
        </id>
        <property name="name" type="text">
            <column name="prod_name"/>
        </property>
        <property name="brand" type="text">
            <column name="brand"/>
        </property>
        <property name="description" type="text">
            <column name="description"/>
        </property>
        <property name="imageUrl" type="text">
            <column name="image_url"/>
        </property>
    </class>
</hibernate-mapping>
